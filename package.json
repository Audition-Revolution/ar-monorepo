{
  "name": "ar-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace ar-client start",
    "server": "yarn workspace server-ar start:dev",
    "start": "lerna run start:dev --parallel",
    "build": "echo 'custom build' && cd packages/server-ar && npm run build"
  },
  "dependencies": {
    "concurrently": "^5.1.0"
  },
  "devDependencies": {
    "lerna": "^3.20.2"
  }
}
