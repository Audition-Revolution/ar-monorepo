{
    "name": "server-ar",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "main": "./dist/main.js",
    "scripts": {
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:dev": "nodemon",
        "start:dev:coverage": "NODE_ENV='production' nyc --silent node ./dist/main.js",
        "start:debug": "nodemon --config nodemon-debug.json",
        "start": "NODE_ENV='production' node ./dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": " ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm"
    },
    "dependencies": {
        "@cypress/code-coverage": "^1.10.2",
        "@nestjs/common": "^6.10.12",
        "@nestjs/core": "^6.10.12",
        "@nestjs/graphql": "^6.5.3",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.0",
        "@nestjs/platform-express": "^6.10.1",
        "@nestjs/platform-socket.io": "^6.10.13",
        "@nestjs/swagger": "^3.1.0",
        "@nestjs/typeorm": "^6.2.0",
        "@nestjs/websockets": "^6.10.13",
        "@sendgrid/mail": "^7.0.1",
        "@sentry/node": "^5.13.1",
        "apollo-server-express": "^2.9.12",
        "aws-sdk": "^2.578.0",
        "axios": "^0.19.0",
        "chargebee": "^2.5.5",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.12.1",
        "concurrently": "^5.0.0",
        "crypto": "^1.0.1",
        "csvtojson": "^2.0.10",
        "date-fns": "^2.8.1",
        "dotenv": "^8.2.0",
        "graphql": "^14.5.8",
        "graphql-tools": "^4.0.6",
        "graphql-upload": "^8.1.0",
        "hbs": "^4.0.6",
        "heapdump": "^0.3.15",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "nestjs-config": "^1.4.4",
        "passport": "^0.4.0",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "pdfkit": "^0.10.0",
        "pg": "^8.3.3",
        "pm2": "^4.1.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.4",
        "swagger-ui-express": "^4.1.2",
        "ts-node": "^8.5.2",
        "twilio": "^3.37.1",
        "type-graphql": "^0.17.6",
        "typedi": "^0.8.0",
        "typeorm": "^0.2.20",
        "typescript": "^3.7.2",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.10.12",
        "@types/express": "^4.17.2",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.12",
        "@types/pdfkit": "^0.10.4",
        "@types/socket.io": "^2.1.4",
        "@types/supertest": "^2.0.8",
        "csv-parser": "^2.3.2",
        "jest": "^24.9.0",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "^24.2.0",
        "tsconfig-paths": "^3.9.0",
        "tslint": "5.20.1"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "ignore": [
            "src/**/*.spec.ts"
        ],
        "exec": "ts-node -r tsconfig-paths/register src/main.ts"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
